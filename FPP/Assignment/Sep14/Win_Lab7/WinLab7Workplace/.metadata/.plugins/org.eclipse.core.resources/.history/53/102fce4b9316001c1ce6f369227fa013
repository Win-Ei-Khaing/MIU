package prog7_3;

public class BinarySearch {

	public static void main(String[] args) {
		System.out.println(search("Hello",'e'));
	}

	static Boolean search(String s, char c) {
		if(s==null || s=="" || c==null) return false;
		int midIndex=s.length()/2;
		char ch=s.charAt(midIndex);
		if(ch==c) return true;
		if(c<ch) search(s.substring(0,midIndex-1), c);
		if(c<ch) search(s.substring(midIndex+1), c);
		return false;
	}
}
