package prog4_3.employeeinfo;

public class CheckingAccount extends Account {
	public CheckingAccount(Employee emp, double balance) {
		super(emp, balance);
	}

	public CheckingAccount(Employee emp) {
		super(emp);
	}

	public AccountType getAcctType() {
		return AccountType.CHECKING;
	}
	
	public double getBalance() {
		double baseBalance = super.getBalance();
		return baseBalance - 5;
	}
	
	public String getNamesOfAccounts() {
		return AccountType.CHECKING.toString().toLowerCase();
	}
}
