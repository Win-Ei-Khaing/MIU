package lab4_6.case2;

import java.util.GregorianCalendar;

public class PersonWithJob extends Person {
	private double salary;

	public PersonWithJob(String n, GregorianCalendar dob, double s) {
		super(n, dob);
		this.salary=s;
	}
	
	public double getSalary() {
		return salary;
	}

	@Override
	public boolean equals(Object aPerson) {
		System.out.println("Equals checking in sub");
		if(aPerson == null)
			return false;
		if(aPerson.getClass() != this.getClass())
			return false;
		
		Person p=(Person)aPerson;
		boolean isEqual=this.name.equals(p.name) && this.dateOfBirth.equals(p.dateOfBirth);
		return isEqual;
		
	}
}
