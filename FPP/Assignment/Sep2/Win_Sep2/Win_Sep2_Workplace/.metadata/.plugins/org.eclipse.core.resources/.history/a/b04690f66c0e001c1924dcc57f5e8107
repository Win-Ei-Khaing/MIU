package prog4;

public class Main {

	public static void main(String[] args) {
		Triangle tri=new Triangle(4.0,6.0);
		System.out.println("Area of Triangle is " + tri.computeArea(tri.getBase(), tri.getHeight()));
		
		Rectangle rec=new Rectangle(6.0,6.0);
		System.out.println("Area of Rectangle is " + rec.computeArea(rec.getWidth(), rec.getLength()));
		
		Circle cir=new Circle(8.0);
		System.out.println("Area of Circle is " + cir.computeArea(cir.getRadius()));
	}

}
