package list;

import java.util.StringJoiner;

public class SinglyList<E> {
	Node<E> head;
	
	
	private static class Node<E>{
		E data;
		Node<E> next;

		Node(E data, Node<E> next){
			this.data=data;
			this.next=next;
		}
		
		String toString() {
			StringJoiner sj=new StringJoiner(", ","[","]");
			while(next!=null) {
				sj.add(next.data.toString());
				next=next.next;
			}
			return sj.toString();
		}
	}
}
