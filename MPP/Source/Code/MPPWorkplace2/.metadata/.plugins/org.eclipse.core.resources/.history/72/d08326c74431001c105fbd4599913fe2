import java.time.LocalDate;
import java.util.Comparator;
import java.util.List;
import java.util.stream.Collectors;

public class Main {
	public static void main(String[] args) {
		List<CheckoutRecord> rList = TestData.INSTANCE.getAllRecords();

		System.out.println(rList);
		List<CheckoutRecord> s = rList.stream()
				.filter(r->r.getCheckoutRecordEntries().stream().filter(e->e.getCheckoutDate().isEqual(LocalDate.of(2014, 06, 22))).findAny().isPresent()
					)
				//.sorted(Comparator.comparing((CheckoutRecord c)->c.get))
				//.map(x->x.getCopy().getBook().getTitle())
				.collect(Collectors.toList());
		System.out.println(s);

	}
}
