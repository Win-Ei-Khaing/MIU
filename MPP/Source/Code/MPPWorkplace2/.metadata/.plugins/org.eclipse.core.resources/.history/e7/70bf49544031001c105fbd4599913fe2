import java.time.LocalDate;
import java.util.Comparator;
import java.util.List;
import java.util.stream.Collectors;

public class Main {
	public static void main(String[] args) {
		List<CheckoutRecord> rList = TestData.INSTANCE.getAllRecords();
		
		System.out.println(rList);
		List<String> s = rList.stream()
				.filter(r->r.getCheckoutRecordEntries()
						.stream()
						.filter(e->e.getCheckoutDate().isEqual(LocalDate.of(2011, 12, 22)))))
						.count()>0)
				//.sorted(Comparator.comparing((CheckoutRecord c)->c.get))
				//map(x->x.getCopy().getBook().getTitle()))
				.collect(Collectors.toList()));
				System.out.println(s);
		
	}
}
